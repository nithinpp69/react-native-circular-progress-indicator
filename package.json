    "react",
    "native",
    "animated",
    "reanimated",
    "chart",
    "ui",
    "animated-chart",
    "progress-indicator",
    "animated-progress-indicator",
    "animated-progress-bar",
    "circular-progress-bar",
    "circular-progress",
    "circular-progress-indicator",
    "mobile",
    "android",
    "ios"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
